{
  "name": "react-redux-entry-input",
  "version": "0.1.3",
  "description": "Input component, in which user can add entries from given immutable set. Provides suggestions as user types.",
  "main": "react-redux-entry-input.js",
  "scripts": {
    "build": "webpack  --progress --colors",
    "prebuild": "npm run lint",
    "lint": "eslint ./src/*.jsx ./src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tohalla/react-enty-input"
  },
  "keywords": [
    "react",
    "redux",
    "entries",
    "suggestions",
    "autosuggest",
    "immutable",
    "component"
  ],
  "peerDependencies": {
    "react": "^0.14.5"
  },
  "devDependencies": {
    "babel": "^5.3.3",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-class-properties": "^6.3.13",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-google": "^0.3.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-plugin-react": "^3.13.1",
    "react": "^0.14.5",
    "uglify-loader": "^1.3.0",
    "webpack": "^1.9.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "syntax-class-properties",
      "transform-class-properties"
    ]
  },
  "author": "Touko Hallasmaa",
  "license": "MIT"
}
